{
  "name": "sshfs-mount",
  "version": "2.0.0",
  "description": "Use SSHFS to mount a remote file system over SSH. Only supports public and private keys. Does not support passwords. Currently only OSX support.",
  "scripts": {
    "build": "tsc",
    "serve": "node dist/index.js",
    "start": "npm run serve",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allencoded/sshfs-mount.git"
  },
  "keywords": [
    "sshfs-mount",
    "sshfs",
    "node",
    "sshfs",
    "osx",
    "ssh"
  ],
  "type": "module",
  "author": "Allen Hendricks",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/allencoded/sshfs-mount/issues"
  },
  "homepage": "https://github.com/allencoded/sshfs-mount#readme",
  "devDependencies": {
    "@types/node": "^16.11.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "typescript": "^4.4.4"
  },
  "declaration": true
}
